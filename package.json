{
  "name": "helloworld",
  "displayName": "hello",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ettk.engTypeToKor",
    "onCommand:ettk.korTypeToEng"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ettk.engTypeToKor",
        "title": "영어 키보드 입력을 한글로"
      },
      {
        "command": "ettk.korTypeToEng",
        "title": "한글 키보드 입력을 영어로"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "ettk.engTypeToKor",
          "when": "editorTextFocus"
        },
        {
          "command": "ettk.korTypeToEng",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
